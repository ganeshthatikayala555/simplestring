public class HelloWorld{
    public static void call(String s){
        int array[]=new int[256];
        for(char c:s.toCharArray()){
           int index=(int)c;
           array[index]++;
        }
        for(int i=0;i<array.length;i++){
            if(array[i]>1){
        System.out.println((char)i+" "+array[i]);
            }
        }
    }
     public static void main(String []args){
        String s="hehllo";
        call(s);
     }
}

